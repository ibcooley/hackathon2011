    <script type="text/JavaScript"> 
      amznJQ.available('jQuery', function() {
        var widgetName  = "click_within_above";
        var homeBN      = /n:1055398/;
        var sportingBN  = /n:3375251/;
        var lazyLoadURL = '/gp/product-ads/lazyLoad/handler/click-within-handler.html';
        var ClickWithin = {};
        ClickWithin.kw = '';
        ClickWithin.sa = '';
        ClickWithin.bl = '';
        amznJQ.available('search-js-general', function() {
          SPUtils.afterEvent('spATFEvent', function() {
            amznJQ.onReady('lazyLoadLib', function () {
              var bl = unescape(jQuery('#refinements').attr('data-browseladder'));
              var kw = iss.keyword();
              var sa = iss.searchAlias();
              if ((kw == ClickWithin.kw && sa == ClickWithin.sa && bl == ClickWithin.bl) || /* search page didn't change */
                  !kw ||       /* no search terms => no ads to fetch */
                  !bl.match(homeBN))  /* we are not under 'Home & Kitchen' */
              {
                  return;
              }
              ClickWithin.sa = sa;
              ClickWithin.kw = kw;
              ClickWithin.bl = bl;
              jQuery('#click_withinLazyLoad_above').lazyLoadContent({
                  threshold : 800,
                  trigger: ['scroll'],
                  url: lazyLoadURL + "?searchTerms="+escape(ClickWithin.kw)+"&searchAlias="+escape(ClickWithin.sa)+
                                     "&isDebug=0&widgetName="+escape(widgetName)+
                                     "&pageType=Search&subPageType="+
                                     "&browseLadder="+escape(ClickWithin.bl)+
                                     "&placement=above",
                  metrics : true,
                  name : widgetName,
                  cache : false
              });
            });
          });
        });
      });
    </script> 
 
    <div id="click_withinLazyLoad_above"> 
    </div> 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
