
    <FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" android:id="@+id/frameLayout1" android:layout_width="fill_parent" android:background="@drawable/main_no_buttons" android:layout_height="fill_parent">
        <RelativeLayout android:id="@+id/relativeLayout1" android:layout_width="wrap_content" android:layout_gravity="center|bottom" android:layout_height="266dp">
        <Button android:layout_width="wrap_content" android:id="@+id/devicesButton" android:background="@drawable/devices_button" android:gravity="center" android:clickable="true" android:layout_height="50dp">></Button>
        <Button android:layout_below="@+id/devicesButton" android:gravity="bottom" android:layout_width="wrap_content" android:id="@+id/roomsButton" android:layout_height="50dip" android:background="@drawable/rooms_button" android:padding="50sp" android:layout_marginTop="10sp" android:clickable="true">></Button>
        <Button android:layout_below="@+id/roomsButton" android:gravity="bottom" android:layout_width="wrap_content" android:id="@+id/settingsButton" android:layout_height="50dip" android:background="@drawable/settings_button" android:padding="50sp" android:layout_marginTop="10sp" android:clickable="true"></Button>
